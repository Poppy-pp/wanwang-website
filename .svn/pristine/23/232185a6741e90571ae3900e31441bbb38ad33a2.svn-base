    <!-- 滚动数字 -->
    <script src="./js/count.js" ></script>
    <script type="text/javascript">
      $(function(){
        var today = new Date().getTime();//获取当前时间
        var space = today - 1513699200000;//获取时间间隔 以2017-12-20-00为起始时间
        var minute = Math.floor(space/1000/60);//将间隔 转换为分钟数-除以毫秒/除以秒
        
        var addCount1 = Math.floor(minute/4);//获取数字一 累加的数字
        var addCount2 = Math.floor(minute/12);//获取数字二 累加的数字

        $(".count1").text(parseInt($(".count1").text()) + parseInt(addCount1));
        $(".count2").text(parseInt($(".count2").text()) + parseInt(addCount2));

        numPlus($(".count1"),240000);//数字一每4分钟增加
        numPlus($(".count2"),720000);  //数字一每12分钟增加

        // 滚动效果
        $(".count1").numberRock({
          speed:10,
          count:$(".count1").text()
        });
        $(".count2").numberRock({
          speed:10,
          count:$(".count2").text()
        })
      });

      // 增加定时器
      var numPlus = function(obj,time){
          setInterval(function(){
              if($(obj).html()==0){
                  return;
              }
              $(obj).html(parseInt($(obj).html())+1)
          },time)
      }

      
    </script>

